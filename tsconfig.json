{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/*": ["./src/*"]
    },
    "plugins": [
      {
        "name": "next",
        "paths": {
          "@/*": ["./src/*"]
        }
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/(sistema)/cuestionario/components/BloqueSalud.tsx.back", "src/app/(sistema)/cuestionario/components/BloqueDatosPersonales.tsx.back", "src/app/(sistema)/cuestionario/components/BloqueEducacion.tsx.back", "src/app/(sistema)/cuestionario/components/BloqueFamiliares.tsx.back", "src/app/(sistema)/cuestionario/components/BloqueSeguridad.tsx.back", "src/app/(sistema)/cuestionario/components/BloqueJudicial.tsx.back"],
  "exclude": ["node_modules"]
}
